{
    "name": "faustbrian/laravel-social-share",
    "description": "",
    "keywords": ["laravel", "framework", "Laravel-social-share", "Laravel Social Share", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "faustbrian/laravel-service-provider": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\SocialShare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\SocialShare\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\SocialShare\\SocialShareServiceProvider"
            ],
            "aliases": {
                "SocialShare": "BrianFaust\\SocialShare\\Facades\\SocialShare"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
